
<div class="main-panel">
  <div class="content-wrapper">
    <div class="col-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title">Edit Property</h4>
            <p class="card-description">
                Edit Property
            </p>
            <p style="color: red;"><%= error %></p>
            <form action="/edit-property/<%= prop._id %>?_method=PATCH" method="post" class="forms-sample">
                <div class="form-group">
                    <label for="name">Name of Property</label>
                    <input type="text" class="form-control" id="exampleInputPassword4" name="name" placeholder="Name" value="<%= prop.name %>" required>
                  </div>
              <div class="form-group">
                <label for="cost">Cost</label>
                <input type="number" class="form-control" id="exampleInputPassword4" name="cost" placeholder="Cost" value="<%= prop.cost %>" required>
              </div>
              <button type="submit" class="btn btn-primary mr-2">Submit</button>
              <a class="btn btn-light" href="/">Cancel</a>
            </form>
          </div>
        </div>
      </div>

</div>
</div>
